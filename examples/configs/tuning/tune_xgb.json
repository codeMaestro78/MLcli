      "max_depth": {"type": "int", "low": 3, "high": 12},
      "learning_rate": {"type": "loguniform", "low": 0.01, "high": 0.3},
      "subsample": {"type": "uniform", "low": 0.6, "high": 1.0},
      "colsample_bytree": {"type": "uniform", "low": 0.6, "high": 1.0}
    }
  },
  "output": {
    "model_dir": "models",
    "save_formats": ["pickle"]
  }
