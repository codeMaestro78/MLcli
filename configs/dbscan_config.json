{
  "dataset": {
    "path": "data/sample_data.csv",
    "type": "csv",
    "target_column": "target"
  },
  "model": {
    "type": "dbscan",
    "params": {
      "eps": 0.5,
      "min_samples": 5,
      "metric": "euclidean",
      "algorithm": "auto",
      "leaf_size": 30,
      "n_jobs": -1
    }
  },
  "training": {
    "test_size": 0.2,
    "random_state": 42,
    "shuffle": true
  },
  "output": {
    "model_dir": "mlcli/models",
    "save_format": ["pickle", "joblib"],
    "log_dir": "runs"
  }
}
